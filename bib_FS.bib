@article{Funmilayo1979,
abstract = {Straw-colored fruit bats are found in isolated colonies comprising millions of individuals in many parts of south-western Nigeria. Roosts are usually situated in groves and thickets close to human habitation, local shrines and marshy areas, which offer sanctuary to the bats. The trees most favored for roosting were those that project above the surrounding vegetation. The straw-colored fruit bat is nocturnal, feeding mainly between 1900 and 0630 h. The diet consists almost exclusively of a variety of ripe succulent fruits and floral parts. Mating and fertilization occur mainly in May to July with about 57-91{\%} of females being pregnant in a breeding season but the fertilized egg does not get implanted until about Nov.-Dec. Young are born in March to May at 39-60 g body wt. Each mother continuously carries about her single baby until June or July when the baby is weaned at 120-150 g body wt. E. helvum is important in the local economy as its roosting activities cause extensive mechanical damage to some economically inportant trees like oil palm (Eleasis guineensis) and Iroko (Chlorophora excelsa). Consumption of fruits like mango, pawpaw, guava, plantain and banana cause yield losses and qualitative devaluation of many fruits. Feces from thousands of bats cause much filth and stench in the environment of some roosts and beneath feeding trees while the clamour of bats in roosts or while feeding near human habitation is very exasperating. Bats are an important item of the diet in all localities where they occur. They also serve as pollinating agents for a number of trees in the families Moraceae and Bombaceae.},
annote = {dc website is under construction rn, check later},
author = {Funmilayo, O},
journal = {Revue de Zoologie Africaine},
number = {3},
pages = {589--600},
title = {{Ecology of the Straw-colored fruit bat Eidolon helvum in Nigeria}},
volume = {93},
year = {1979}
}
